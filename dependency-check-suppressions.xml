    <!--
    Exemples de suppressions pour faux positifs
    À adapter selon vos besoins
    -->
    <suppress base="true">
        <notes><![CDATA[
        Suppression générale pour les vulnérabilités nécessitant une analyse manuelle
        ]]></notes>
        <gav regex="true">.*</gav>
        <cve>CVE-2022-45685</cve>
    </suppress>
    
