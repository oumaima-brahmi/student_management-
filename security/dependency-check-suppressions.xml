<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    <!-- Ignore H2 database vulnerabilities (test only) -->
    <suppress>
        <notes><![CDATA[False positive - Test dependencies only]]></notes>
        <gav regex="true">^com\.h2database:h2:.*$</gav>
    </suppress>
    
    <!-- Ignore Lombok false positives -->
    <suppress>
        <notes><![CDATA[False positive in Lombok]]></notes>
        <gav regex="true">^org\.projectlombok\lombok:.*$</gav>
    </suppress>
</suppressions>
